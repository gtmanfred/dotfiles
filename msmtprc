#Accounts will inherit settings from this section
defaults
auth             on
tls				 on
tls_certcheck    off

account        gatech
host           mail.gatech.edu
port           587
from           daniel.wallace@gatech.edu
user           dwallace3
passwordeval   gd ~/.gnupg/gatech.gpg

account       gtmanfred
host          smtp.gmail.com
port          587
from          danielwallace@gtmanfred.com
user          danielwallace@gtmanfred.com
passwordeval  ~/bin/gd ~/.gnupg/gtmanfred.gpg

# Set a default account
account default : gtmanfred
