GNUPGHOME                   DEFAULT=~/.config/gnupg
HISTFILE                    DEFAULT=~/.cache/history
ZDOTDIR                     DEFAULT=~/.config/zsh
LESSHISTFILE                DEFAULT=~/.cache/lesshist
VIMINIT                     DEFAULT='let $MYVIMRC="~/.config/vim/vimrc"| source $MYVIMRC'
NOVACLIENT_UUID_CACHE_DIR   DEFAULT=~/.cache/novaclient
VIRTUAL_ENV_DISABLE_PROMPT  DEFAULT=1
BROWSER                     DEFAULT=firefox
